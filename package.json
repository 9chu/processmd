{
  "name": "processto",
  "version": "1.0.0",
  "description": "Process a directory of markdown and yaml files to JSON files",
  "main": "index.js",
  "bin": {
    "processto": "./cli.js"
  },
  "scripts": {
    "prettier": "prettier --write --single-quote --trailing-comma --no-semi *.js",
    "start": "node cli.js \"test/data/input/**/*.{yml,md}\" --outputDir test/data/output",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "process",
    "markdown",
    "yaml",
    "json"
  ],
  "author": "Tim Scanlin",
  "license": "MIT",
  "dependencies": {
    "globby": "^6.1.0",
    "js-yaml": "^3.8.3",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "prettier": "^1.2.2"
  }
}
